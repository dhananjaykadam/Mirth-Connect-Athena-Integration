	item.username=patient.username;
	chresponse = router.routeMessage(&quot;DELETE - REMOVE USER FROM BUDDY LIST&quot;, JSON.stringify(item));
}
chresponse = router.routeMessage(&quot;DELETE - DELETE USER&quot;, JSON.stringify(patient));
